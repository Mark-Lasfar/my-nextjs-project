    children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: `w-32 fixed  h-full ${(0, _i18nconfig.getDirection)(locale) === 'rtl' ?
      /* istanbul ignore next */
      (cov_5nhu07vfm().b[2][0]++, 'border-r') :
      /* istanbul ignore next */
      (cov_5nhu07vfm().b[2][1]++, 'border-l')}`,
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "p-2 h-full flex flex-col gap-2 justify-center items-center",
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "text-center space-y-2",
          children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            children: [" ", t('Cart.Subtotal')]
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "font-bold ",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_productprice.default, {
              price: itemsPrice,
              plain: true
            })
          }),
          /* istanbul ignore next */
          (cov_5nhu07vfm().b[3][0]++, itemsPrice > freeShippingMinPrice) &&
          /* istanbul ignore next */
          (cov_5nhu07vfm().b[3][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: " text-center text-xs",
            children: t('Cart.Your order qualifies for FREE Shipping')
          })), /*#__PURE__*/(0, _jsxruntime.jsx)(_link.default, {
            className: (0, _utils.cn)((0, _button.buttonVariants)({
              variant: 'outline'
            }), 'rounded-full hover:no-underline w-full'),
            href: "/cart",
            children: t('Cart.Go to Cart')
          }), /*#__PURE__*/(0, _jsxruntime.jsx)(_separator.Separator, {
            className: "mt-3"
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsx)(_scrollarea.ScrollArea, {
          className: "flex-1  w-full",
          children: items.map(item => {
            /* istanbul ignore next */
            cov_5nhu07vfm().f[3]++;
            cov_5nhu07vfm().s[24]++;
            return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
                className: "my-3",
                children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_link.default, {
                  href: `/product/${item.slug}`,
                  children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                    className: "relative h-24",
                    children: /*#__PURE__*/(0, _jsxruntime.jsx)(_image.default, {
                      src: item.image,
                      alt: item.name,
                      fill: true,
                      sizes: "20vw",
                      className: "object-contain"
                    })
                  })
                }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
                  className: "text-sm text-center font-bold",
                  children: /*#__PURE__*/(0, _jsxruntime.jsx)(_productprice.default, {
                    price: item.price,
                    plain: true
                  })
                }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
                  className: "flex gap-2 mt-2",
                  children: [/*#__PURE__*/(0, _jsxruntime.jsxs)(_select.Select, {
                    value: item.quantity.toString(),
                    onValueChange: value => {
                      /* istanbul ignore next */
                      cov_5nhu07vfm().f[4]++;
                      cov_5nhu07vfm().s[25]++;
                      updateItem(item, Number(value));
                    },
                    children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_select.SelectTrigger, {
                      className: "text-xs w-12 ml-1 h-auto py-0",
                      children: /*#__PURE__*/(0, _jsxruntime.jsx)(_select.SelectValue, {})
                    }), /*#__PURE__*/(0, _jsxruntime.jsx)(_select.SelectContent, {
                      children: Array.from({
                        length: item.countInStock
                      }).map((_, i) => {
                        /* istanbul ignore next */
                        cov_5nhu07vfm().f[5]++;
                        cov_5nhu07vfm().s[26]++;
                        return /*#__PURE__*/(0, _jsxruntime.jsx)(_select.SelectItem, {
                          value: (i + 1).toString(),
                          children: i + 1
                        }, i + 1);
                      })
                    })]
                  }), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
                    variant: 'outline',
                    size: 'sm',
                    onClick: () => {
                      /* istanbul ignore next */
                      cov_5nhu07vfm().f[6]++;
                      cov_5nhu07vfm().s[27]++;
                      removeItem(item);
                    },
                    children: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.TrashIcon, {
                      className: "w-4 h-4"
                    })
                  })]
                })]
              }), /*#__PURE__*/(0, _jsxruntime.jsx)(_separator.Separator, {})]
            }, item.clientId);
          })
        })]
      })
    })
  });
}
