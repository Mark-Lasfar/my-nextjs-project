      cov_2epnnykd21().s[18]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_2epnnykd21().b[2][1]++;
    }
    cov_2epnnykd21().s[19]++;
    setIsLoading(true);
    /* istanbul ignore next */
    cov_2epnnykd21().s[20]++;
    stripe.confirmPayment({
      elements,
      confirmParams: {
        return_url: `${site.url}/checkout/${orderId}/stripe-payment-success`
      }
    }).then(({
      error
    }) => {
      /* istanbul ignore next */
      cov_2epnnykd21().f[4]++;
      cov_2epnnykd21().s[21]++;
      if (
      /* istanbul ignore next */
      (cov_2epnnykd21().b[5][0]++, error.type === 'card_error') ||
      /* istanbul ignore next */
      (cov_2epnnykd21().b[5][1]++, error.type === 'validation_error')) {
        /* istanbul ignore next */
        cov_2epnnykd21().b[4][0]++;
        cov_2epnnykd21().s[22]++;
        setErrorMessage(error.message);
      } else {
        /* istanbul ignore next */
        cov_2epnnykd21().b[4][1]++;
        cov_2epnnykd21().s[23]++;
        setErrorMessage('An unknown error occurred');
      }
    }).finally(() => {
      /* istanbul ignore next */
      cov_2epnnykd21().f[5]++;
      cov_2epnnykd21().s[24]++;
      return setIsLoading(false);
    });
  }
  /* istanbul ignore next */
  cov_2epnnykd21().s[25]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("form", {
    onSubmit: handleSubmit,
    className: "space-y-4",
    children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "text-xl",
      children: "Stripe Checkout"
    }),
    /* istanbul ignore next */
    (cov_2epnnykd21().b[6][0]++, errorMessage) &&
    /* istanbul ignore next */
    (cov_2epnnykd21().b[6][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "text-destructive",
      children: errorMessage
    })), /*#__PURE__*/(0, _jsxruntime.jsx)(_reactstripejs.PaymentElement, {}), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      children: /*#__PURE__*/(0, _jsxruntime.jsx)(_reactstripejs.LinkAuthenticationElement, {
        onChange: e => {
          /* istanbul ignore next */
          cov_2epnnykd21().f[6]++;
          cov_2epnnykd21().s[26]++;
          return setEmail(e.value.email);
        }
      })
    }), /*#__PURE__*/(0, _jsxruntime.jsx)(_button.Button, {
      className: "w-full",
      size: "lg",
      disabled:
      /* istanbul ignore next */
      (cov_2epnnykd21().b[7][0]++, stripe == null) ||
      /* istanbul ignore next */
      (cov_2epnnykd21().b[7][1]++, elements == null) ||
      /* istanbul ignore next */
      (cov_2epnnykd21().b[7][2]++, isLoading),
      children: isLoading ?
      /* istanbul ignore next */
      (cov_2epnnykd21().b[8][0]++, 'Purchasing...') :
      /* istanbul ignore next */
      (/*#__PURE__*/cov_2epnnykd21().b[8][1]++, (0, _jsxruntime.jsxs)("div", {
        children: ["Purchase - ", /*#__PURE__*/(0, _jsxruntime.jsx)(_productprice.default, {
          price: priceInCents / 100,
          plain: true
        })]
      }))
    })]
  });
}
